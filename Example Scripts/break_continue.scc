cycle (i 1 -> 5) {
    foretell (i == 4) {
        shatter
    }
    reveal $"{i}\n"
}

cycle (i 1 -> 5) {
    foretell (i == 4) {
        persist
    }
    reveal $"{i}\n"
}

cycle (i 1 -> 5) {
    reveal "Out "
    cycle (j 1 -> 2) {
        foretell (i == 4) {
            reveal "\n"
            persist
        }
        reveal $"{i}\n"
    }
}